<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:orientation="vertical" android:id="@id/map_root" android:layout_width="fill_parent" android:layout_height="fill_parent"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <com.autonavi.xmgd.view.MapView android:id="@id/mapView" android:layout_width="fill_parent" android:layout_height="fill_parent" />
    <RelativeLayout android:id="@id/map_combinetitle" android:layout_width="fill_parent" android:layout_height="wrap_content">
        <RelativeLayout android:id="@id/map_title" android:background="@drawable/titlelbar_nav_bg" android:layout_width="fill_parent" android:layout_height="@dimen/map_titlebar_height">
            <ImageButton android:id="@id/map_title_user" android:background="@drawable/ic_btn_user" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/map_titlebar_item_margin" android:layout_alignParentLeft="true" android:layout_centerVertical="true" />
            <ImageButton android:id="@id/map_title_voice" android:background="@drawable/ic_btn_speak" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="@dimen/map_titlebar_item_margin" android:layout_alignParentRight="true" android:layout_centerVertical="true" />
            <com.autonavi.xmgd.view.GDScrollText android:gravity="center" android:id="@id/map_title_roadname" android:layout_width="fill_parent" android:layout_height="fill_parent" android:layout_toLeftOf="@id/map_title_voice" android:layout_toRightOf="@id/map_title_user" />
        </RelativeLayout>
        <include android:id="@id/map_navi_title" android:layout_width="fill_parent" android:layout_height="@dimen/navi_title_vertical" layout="@layout/navi_title" />
        <RelativeLayout android:id="@id/map_sp_title" android:background="@drawable/titlelbar_nav_bg" android:layout_width="fill_parent" android:layout_height="@dimen/map_titlebar_height">
            <ImageButton android:id="@id/map_sp_title_back" android:background="@drawable/gdtitle_back" android:paddingLeft="@dimen/map_titlebar_item_margin" android:paddingRight="@dimen/map_titlebar_item_margin" android:layout_width="wrap_content" android:layout_height="fill_parent" android:src="@drawable/ic_btn_back" android:layout_alignParentLeft="true" android:layout_centerVertical="true" />
            <ImageButton android:id="@id/map_sp_title_right" android:background="@drawable/gdtitle_back" android:paddingLeft="@dimen/map_titlebar_item_margin" android:paddingRight="@dimen/map_titlebar_item_margin" android:visibility="invisible" android:layout_width="wrap_content" android:layout_height="fill_parent" android:src="@drawable/ic_btn_back" android:layout_alignParentRight="true" android:layout_centerVertical="true" />
            <TextView android:textSize="20.0sp" android:textColor="#ffffffff" android:gravity="center" android:id="@id/map_sp_title_name" android:layout_width="fill_parent" android:layout_height="fill_parent" android:text="@string/mapsp_title_name" android:layout_toLeftOf="@id/map_sp_title_right" android:layout_toRightOf="@id/map_sp_title_back" />
        </RelativeLayout>
    </RelativeLayout>
    <TextView android:textSize="18.0dip" android:textColor="#ffff00ff" android:id="@id/map_testversion" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="@dimen/map_right_item_margin" android:text="这是测试版本!" android:layout_below="@id/map_combinetitle" android:layout_alignParentLeft="true" />
    <com.autonavi.xmgd.view.ViewmodeDrawer android:id="@id/map_viewmode_switch" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="@dimen/map_right_item_margin" android:layout_below="@id/map_combinetitle" android:layout_alignParentRight="true" />
    <com.autonavi.xmgd.view.GDImageButton android:id="@id/map_layer" android:background="@drawable/btn_layer" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/map_right_item_margin" android:layout_marginRight="@dimen/map_right_item_margin" android:layout_marginBottom="@dimen/map_right_item_margin" android:layout_below="@id/map_viewmode_switch" android:layout_alignParentRight="true" />
    <include android:id="@id/map_toolbar_view" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginBottom="@dimen/map_toolbar_toolbar_marginBottom" android:layout_alignParentBottom="true" android:layout_centerHorizontal="true" layout="@layout/map_toolbar" />
    <include android:id="@id/map_toolbar_sim_view" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_alignParentBottom="true" android:layout_centerHorizontal="true" layout="@layout/map_toolbar_simulate" />
    <include android:id="@id/map_toolbar_showmap_view" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_alignParentBottom="true" android:layout_centerHorizontal="true" layout="@layout/map_toolbar_showmap" />
    <com.autonavi.xmgd.view.GDImageButton android:id="@id/map_rt_broadcast" android:background="@drawable/btn_broadcast" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="@dimen/map_right_item_margin" android:layout_above="@id/map_toolbar_view" android:layout_alignParentRight="true" />
    <com.autonavi.xmgd.view.GDImageButton android:id="@id/map_call_center" android:background="@drawable/btn_onekey" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="@dimen/map_right_item_margin" android:layout_above="@id/map_toolbar_view" android:layout_alignParentLeft="true" />
    <TextView android:textSize="@dimen/map_scale_text_size" android:gravity="center_horizontal" android:id="@id/map_scale_level" android:background="@drawable/scale_icon" android:paddingBottom="@dimen/map_scale_text_paddingBottom" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="@dimen/map_right_item_margin" android:layout_above="@id/map_rt_broadcast" android:layout_alignParentRight="true" />
    <com.autonavi.xmgd.view.GDImageButton android:id="@id/map_recorder" android:background="@drawable/btn_recorder" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="@dimen/map_right_item_margin" android:layout_above="@id/map_call_center" android:layout_alignParentLeft="true" />
    <com.autonavi.xmgd.view.GDZoomButton android:id="@id/map_zoom" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="@dimen/map_right_item_margin" android:layout_alignParentRight="true" android:layout_centerVertical="true" />
    <include android:id="@id/routes_manage_view" android:layout_width="fill_parent" android:layout_height="fill_parent" layout="@layout/routes_manage" />
    <include android:id="@id/show_cross_view" android:layout_width="fill_parent" android:layout_height="fill_parent" layout="@layout/map_show_cross" />
</RelativeLayout>