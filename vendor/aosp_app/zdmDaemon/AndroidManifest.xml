        </receiver>
        <receiver android:name="com.zte.zdmdaemon.ZdmDaemonReceiver" android:permission="android.permission.ZDM_RECOVERY_REBOOT">
            <intent-filter>
                <action android:name="android.intent.action.RECOVERY_REBOOT" />
            </intent-filter>
        </receiver>
    </application>
</manifest>