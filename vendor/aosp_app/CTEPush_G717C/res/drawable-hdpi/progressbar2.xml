<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@*android:id/background">
        <shape>
            <corners android:radius="5.0dip" />
            <gradient android:startColor="#fff28226" android:endColor="#fff28226" android:angle="270.0" android:centerY="0.75" android:centerColor="#fff28226" />
        </shape>
    </item>
    <item android:id="@*android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#80ffd300" android:endColor="#a0ffcb00" android:angle="270.0" android:centerY="0.75" android:centerColor="#80ffb600" />
            </shape>
        </clip>
    </item>
    <item android:id="@*android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#ffbccb85" android:endColor="#ffbccb85" android:angle="270.0" android:centerY="0.75" android:centerColor="#ffbccb85" />
            </shape>
        </clip>
    </item>
</layer-list>