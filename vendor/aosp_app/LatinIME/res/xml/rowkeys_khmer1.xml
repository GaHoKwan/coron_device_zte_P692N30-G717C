<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <switch>
        <case latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted">
            <Key latin:keyLabel="ឈ" />
            <Key latin:keyLabel="ៃ" />
            <Key latin:keyLabel="ែ" />
            <Key latin:moreKeys="ឫ" latin:keyLabel="ឬ" />
            <Key latin:keyLabel="ទ" />
            <Key latin:keyLabel="ួ" />
            <Key latin:keyLabel="ូ" />
            <Key latin:keyLabel="គ" />
            <Key latin:keyLabel="ឡ" />
            <Key latin:keyLabel="ភ" />
        </case>
        <default>
            <Key latin:additionalMoreKeys="1,១" latin:keyLabel="ឆ" latin:keyHintLabel="១" />
            <Key latin:additionalMoreKeys="2,២" latin:keyLabel="ស" latin:keyHintLabel="២" />
            <Key latin:additionalMoreKeys="3,៣" latin:keyLabel="េ" latin:keyHintLabel="៣" />
            <Key latin:moreKeys="៛" latin:additionalMoreKeys="4,៤" latin:keyLabel="រ" latin:keyHintLabel="៤" />
            <Key latin:moreKeys="ឝ,ឥ" latin:additionalMoreKeys="5,៥" latin:keyLabel="ត" latin:keyHintLabel="៥" />
            <Key latin:additionalMoreKeys="6,៦" latin:keyLabel="យ" latin:keyHintLabel="៦" />
            <Key latin:moreKeys="់" latin:additionalMoreKeys="7,៧" latin:keyLabel="ុ" latin:keyHintLabel="៧" />
            <Key latin:additionalMoreKeys="8,៨" latin:keyLabel="ក" latin:keyHintLabel="៨" />
            <Key latin:moreKeys="ឦ" latin:additionalMoreKeys="9,៩" latin:keyLabel="ល" latin:keyHintLabel="៩" />
            <Key latin:additionalMoreKeys="0,០" latin:keyLabel="ផ" latin:keyHintLabel="០" />
        </default>
    </switch>
</merge>