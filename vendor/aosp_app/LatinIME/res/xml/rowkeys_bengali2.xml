<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <switch>
        <case latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted">
            <Key latin:keyLabel="ো" />
            <Key latin:keyLabel="ে" />
            <Key latin:moreKeys="ৄ" latin:keyLabel="ৃ" />
            <Key latin:keyLabel="ি" />
            <Key latin:keyLabel="ু" />
            <Key latin:keyLabel="প" />
            <Key latin:keyLabel="য" />
            <Key latin:keyLabel="ক" />
            <Key latin:keyLabel="ত" />
        </case>
        <default>
            <Key latin:keyLabel="ও" />
            <Key latin:keyLabel="এ" />
            <Key latin:moreKeys="ঠ" latin:keyLabel="অ" />
            <Key latin:keyLabel="ই" />
            <Key latin:keyLabel="উ" />
            <Key latin:keyLabel="ফ" />
            <Key latin:keyLabel="য়" />
            <Key latin:keyLabel="খ" />
            <Key latin:keyLabel="থ" />
        </default>
    </switch>
</merge>