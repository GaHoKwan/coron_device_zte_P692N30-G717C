<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <switch>
        <case latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted">
            <Key latin:keyLabel="ဗ" />
            <Key latin:keyLabel="ှ" />
            <Key latin:keyLabel="ီ" />
            <Key latin:keyLabel="ဤ" />
            <Key latin:moreKeys="ႛ,ႆ,ႇ,\uaa7b" latin:keyLabel="ံ" />
            <Key latin:moreKeys="ဴ,ၠ,ၳ,ႍ" latin:keyLabel="ဲ" />
            <Key latin:keyLabel="၊" />
            <Key latin:keyLabel="၂" />
            <Key latin:moreKeys="ၮ,\uaa6c" latin:keyLabel="ဏ" />
            <Key latin:moreKeys="၆,ဨ" latin:keyLabel="၉" />
        </case>
        <default>
            <Key latin:moreKeys="ေ,ၘ,ၙ" latin:keyLabel="ဵ" />
            <Key latin:moreKeys="ၧ,ၨ,ၪ" latin:keyLabel="ျ" />
            <Key latin:moreKeys="ဳ" latin:keyLabel="ိ" />
            <Key latin:moreKeys="ႂ,ာ" latin:keyLabel="်" />
            <Key latin:moreKeys="ံ,ႉ,ႋ,ႚ" latin:keyLabel="့" />
            <Key latin:moreKeys="္" latin:keyLabel="ြ" />
            <Key latin:keyLabel="ု" />
            <Key latin:moreKeys="ၢ,ၤ,ၬ,ႃ" latin:keyLabel="ူ" />
            <Key latin:moreKeys="ၱ,ႌ,ႏ,ႈ,ႊ" latin:keyLabel="း" />
            <Key latin:moreKeys="၊" latin:keyLabel="။" />
        </default>
    </switch>
</merge>