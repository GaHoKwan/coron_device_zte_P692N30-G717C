<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <switch>
        <case latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted">
            <Key latin:moreKeys="៕" latin:keyLabel="។" />
            <Key latin:keyLabel="ឺ" />
            <Key latin:keyLabel="ឌ" />
            <Key latin:keyLabel="ធ" />
            <Key latin:keyLabel="អ" />
            <Key latin:moreKeys="ៈ" latin:keyLabel="ះ" />
            <Key latin:keyLabel="ញ" />
            <Key latin:keyLabel="ី" />
            <Key latin:keyLabel="ៅ" />
            <Key latin:moreKeys="ឭ,ឰ,ៗ" latin:keyLabel="ឮ" />
        </case>
        <default>
            <Key latin:keyLabel="ា" />
            <Key latin:keyLabel="ឹ" />
            <Key latin:keyLabel="ដ" />
            <Key latin:moreKeys="ឲ" latin:keyLabel="ថ" />
            <Key latin:moreKeys="ឯ" latin:keyLabel="ង" />
            <Key latin:keyLabel="ហ" />
            <Key latin:moreKeys="៎" latin:keyLabel="្" />
            <Key latin:moreKeys="៊,៌,៍,៏,័" latin:keyLabel="ិ" />
            <Key latin:keyLabel="ោ" />
            <Key latin:keyLabel="ើ" />
        </default>
    </switch>
</merge>