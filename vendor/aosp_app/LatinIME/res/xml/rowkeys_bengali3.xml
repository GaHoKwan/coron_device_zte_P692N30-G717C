<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <switch>
        <case latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted">
            <Key latin:keyLabel="জ" />
            <Key latin:keyLabel="ং" />
            <Key latin:keyLabel="ম" />
            <Key latin:keyLabel="ন" />
            <Key latin:moreKeys="ট,ঢ" latin:keyLabel="চ" />
            <Key latin:keyLabel="ল" />
            <Key latin:keyLabel="স" />
        </case>
        <default>
            <Key latin:keyLabel="ঝ" />
            <Key latin:keyLabel="ঁ" />
            <Key latin:keyLabel="ণ" />
            <Key latin:keyLabel="।" />
            <Key latin:keyLabel="ছ" />
            <Key latin:keyLabel="র" />
            <Key latin:keyLabel="শ" />
        </default>
    </switch>
</merge>