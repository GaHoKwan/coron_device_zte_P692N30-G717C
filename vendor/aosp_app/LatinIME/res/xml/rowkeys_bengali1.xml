<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <switch>
        <case latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted">
            <Key latin:additionalMoreKeys="1,১" latin:keyLabel="ৌ" latin:keyHintLabel="1" />
            <Key latin:additionalMoreKeys="2,২" latin:keyLabel="ৈ" latin:keyHintLabel="2" />
            <Key latin:additionalMoreKeys="3,৩" latin:keyLabel="া" latin:keyHintLabel="3" />
            <Key latin:additionalMoreKeys="4,৪" latin:keyLabel="ী" latin:keyHintLabel="4" />
            <Key latin:additionalMoreKeys="5,৫" latin:keyLabel="ূ" latin:keyHintLabel="5" />
            <Key latin:additionalMoreKeys="6,৬" latin:keyLabel="ব" latin:keyHintLabel="6" />
            <Key latin:additionalMoreKeys="7,৭" latin:keyLabel="হ" latin:keyHintLabel="7" />
            <Key latin:additionalMoreKeys="8,৮" latin:keyLabel="গ" latin:keyHintLabel="8" />
            <Key latin:additionalMoreKeys="9,৯" latin:keyLabel="দ" latin:keyHintLabel="9" />
            <Key latin:additionalMoreKeys="0,০" latin:keyLabel="ঞ" latin:keyHintLabel="0" />
        </case>
        <default>
            <Key latin:additionalMoreKeys="1,১" latin:keyLabel="ঔ" latin:keyHintLabel="1" />
            <Key latin:additionalMoreKeys="2,২" latin:keyLabel="ঐ" latin:keyHintLabel="2" />
            <Key latin:additionalMoreKeys="3,৩" latin:keyLabel="আ" latin:keyHintLabel="3" />
            <Key latin:additionalMoreKeys="4,৪" latin:keyLabel="ঈ" latin:keyHintLabel="4" />
            <Key latin:additionalMoreKeys="5,৫" latin:keyLabel="ঊ" latin:keyHintLabel="5" />
            <Key latin:additionalMoreKeys="6,৬" latin:keyLabel="ভ" latin:keyHintLabel="6" />
            <Key latin:additionalMoreKeys="7,৭,ড" latin:keyLabel="ঙ" latin:keyHintLabel="7" />
            <Key latin:additionalMoreKeys="8,৮" latin:keyLabel="ঘ" latin:keyHintLabel="8" />
            <Key latin:additionalMoreKeys="9,৯" latin:keyLabel="ধ" latin:keyHintLabel="9" />
            <Key latin:additionalMoreKeys="0,০" latin:keyLabel="ষ" latin:keyHintLabel="0" />
        </default>
    </switch>
</merge>