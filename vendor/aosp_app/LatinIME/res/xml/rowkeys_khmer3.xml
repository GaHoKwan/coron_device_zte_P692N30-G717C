<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <switch>
        <case latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted">
            <Key latin:keyLabel="ឍ" />
            <Key latin:keyLabel="ឃ" />
            <Key latin:keyLabel="ជ" />
            <Key latin:moreKeys="ឿ" latin:keyLabel="ៀ" />
            <Key latin:keyLabel="ព" />
            <Key latin:keyLabel="ណ" />
            <Key latin:moreKeys="៓" latin:keyLabel="ំ" />
        </case>
        <default>
            <Key latin:keyLabel="ឋ" />
            <Key latin:keyLabel="ខ" />
            <Key latin:keyLabel="ច" />
            <Key latin:keyLabel="វ" />
            <Key latin:moreKeys="ឞ" latin:keyLabel="ប" />
            <Key latin:keyLabel="ន" />
            <Key latin:keyLabel="ម" />
        </default>
    </switch>
</merge>