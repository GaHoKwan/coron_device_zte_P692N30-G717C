<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <switch>
        <case latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted">
            <Key latin:moreKeys="ၜ,\uaa6f" latin:keyLabel="၀" />
            <Key latin:keyLabel="ရ" />
            <Key latin:moreKeys="ၐ,ၵ,ႁ,ၷ,\uaa6b" latin:keyLabel="ဂ" />
            <Key latin:keyLabel="၏" />
            <Key latin:keyLabel="၍" />
            <Key latin:keyLabel="ဉ" />
            <Key latin:keyLabel="၅" />
            <Key latin:keyLabel="ဒ" />
            <Key latin:moreKeys="ႎ" latin:keyLabel="ဓ" />
            <Key latin:moreKeys="ၯ" latin:keyLabel="ဟ" />
        </case>
        <default>
            <Key latin:moreKeys="ၸ,ၹ,\uaa61,\uaa63" latin:additionalMoreKeys="1,႑" latin:keyLabel="ဆ" latin:keyHintLabel="႑" />
            <Key latin:moreKeys="ၻ,\uaa68,\uaa66" latin:additionalMoreKeys="2,႒" latin:keyLabel="တ" latin:keyHintLabel="႒" />
            <Key latin:additionalMoreKeys="3,႓" latin:keyLabel="န" latin:keyHintLabel="႓" />
            <Key latin:moreKeys="ၝ,ဧ" latin:additionalMoreKeys="4,႔" latin:keyLabel="မ" latin:keyHintLabel="႔" />
            <Key latin:additionalMoreKeys="5,႕" latin:keyLabel="အ" latin:keyHintLabel="႕" />
            <Key latin:moreKeys="ဒ,ဎ,ၑ,ၒ,ၓ,ၦ" latin:additionalMoreKeys="6,႖" latin:keyLabel="ပ" latin:keyHintLabel="႖" />
            <Key latin:moreKeys="ဢ,\uaa60,\uaa71" latin:additionalMoreKeys="7,႗" latin:keyLabel="က" latin:keyHintLabel="႗" />
            <Key latin:moreKeys="၁,၎,၃,၄" latin:additionalMoreKeys="8,႘" latin:keyLabel="င" latin:keyHintLabel="႘" />
            <Key latin:moreKeys="ဿ,\uaa72" latin:additionalMoreKeys="9,႙" latin:keyLabel="သ" latin:keyHintLabel="႙" />
            <Key latin:moreKeys="ဈ" latin:additionalMoreKeys="0,႐" latin:keyLabel="စ" latin:keyHintLabel="႐" />
        </default>
    </switch>
</merge>