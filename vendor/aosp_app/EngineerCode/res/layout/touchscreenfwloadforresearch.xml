<?xml version="1.0" encoding="utf-8"?>
<FrameLayout android:layout_width="fill_parent" android:layout_height="fill_parent"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <LinearLayout android:gravity="center" android:orientation="vertical" android:layout_width="fill_parent" android:layout_height="fill_parent">
        <Button android:id="@id/btn_tp_info" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/tpinfo" android:layout_centerHorizontal="true" />
        <Button android:id="@id/btn_tp_calibration" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/tpcalibration" android:layout_centerHorizontal="true" />
        <Button android:id="@id/btn_common_download" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/commondownload" android:layout_centerHorizontal="true" />
        <Button android:id="@id/btn_force_download" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/forcedownload" android:layout_centerHorizontal="true" />
        <TextView android:id="@id/tp_info" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="15.0px" android:layout_centerHorizontal="true" />
        <TextView android:id="@id/tp_calibration" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="15.0px" android:layout_centerHorizontal="true" />
        <TextView android:id="@id/update_or_not" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="15.0px" android:layout_centerHorizontal="true" />
        <TextView android:id="@id/common_load" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="15.0px" android:layout_centerHorizontal="true" />
        <TextView android:id="@id/force_load" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="15.0px" android:layout_centerHorizontal="true" />
    </LinearLayout>
    <Button android:layout_gravity="bottom|center" android:id="@id/finishtest" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginBottom="15.0px" android:text="@string/OK" />
</FrameLayout>